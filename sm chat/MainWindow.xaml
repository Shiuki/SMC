<Window x:Class="sm_chat.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:sm_chat"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Background="#FF292626" Foreground="Gainsboro">
    <Grid>
        <Button x:Name="button" Content="Button" HorizontalAlignment="Left" Margin="10,374,0,0" VerticalAlignment="Top" Width="75" Background="#FF62B73A"/>
        <StackPanel HorizontalAlignment="Left" Height="301" Margin="55,57,0,0" VerticalAlignment="Top" Width="659">
            <TextBlock x:Name="textBlock" TextWrapping="Wrap" Text="TextBlock"/>
            <TextBlock x:Name="textBlock1" TextWrapping="Wrap" Text="TextBlock1"/>
        </StackPanel>

    </Grid>
</Window>
